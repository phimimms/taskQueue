{
  "name": "taskqueue",
  "version": "0.2.0",
  "description": "A data structure to maintain the order of asynchronous function invocations.",
  "scripts": {
    "prebuild": "rimraf dist && mkdir \"dist\"",
    "build": "npm-run-all build:amd build:es6",
    "build:amd": "babel src/index.js -o dist/taskQueue.amd.js --plugins transform-es2015-modules-amd",
    "build:es6": "babel src/index.js -o dist/taskQueue.js",
    "clean": "rimraf node_modules && npm install",
    "lint": "eslint src",
    "test": "nyc mocha --compilers js:babel-core/register src/index.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phimimms/taskQueue.git"
  },
  "author": "Philippe Mimms",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsdoc": "^3.1.3",
    "expect": "^1.20.2",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.2.1",
    "rimraf": "^2.6.2"
  }
}
